import std
import fileio
import args
import lex

function(int nargs, ArgList args) main -> int does
	if nargs!=2 do
		printf("E: Bad args\n")
		return 1
	done

	File fd = File::open(args:get(1), "r")
	cstr contents=fd:readall()
	fd:close()
	fd:free()

	printf("Parsing Text: `%s`\n", contents)

	lex::init()
	lex::tokenize(contents)
	lex::free()
	contents:free()
return 0