import list
import std
import err

type ListIterator is
	function(ListIterator i) ListIterator:_l -> List does
	return i|List
endtype

function(List l) List:__iter__ -> ListIterator does
return l|ListIterator

function(ListIterator i) ListIterator:__ilen__ -> int does
return i:_l().len

function(ListIterator i, int pos) ListIterator:__iget__ -> ptr does
return i:_l():get(pos)

function(ListIterator i) ListIterator:__idel__ -> void does
return

function() main -> int does
	List l = List::new()
	l:append("Foo")
	l:append("Bar")
	l:append("Baz")

	for item in l do
		printf("Item = %s\n", item)
	done
return 0