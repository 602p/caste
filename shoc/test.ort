
extern(...) malloc -> ptr

type Node is
	int a
	Node next
endtype

function() make_node -> Node does
return malloc(@sizeof(Node)@)|Node

function() main -> int does
	Node head = make_node()
	head.next=make_node()
	head.next.next=make_node()
	head.a=1
	head.next.a=2
	head.next.next.a=3
return head.next.next.a