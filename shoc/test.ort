
type TestFP is a function->int

function(int value) square -> int does
return value*value

function(int value) add1 -> int does
return value+1

function(TestFP func, int val) do_twice -> int does
return func(func(val))

function() main -> int does
	TestFP f = add1|TestFP
	int i = do_twice(f, 1+1)
return i