
function() test_svga -> void does
	(multiboot_data_physaddr:offset(base_vma)|RawMultibootInfo):print()
	ptr start=(base_vma+0xA0000)|ptr
	int depth=3 #bytes_per_pixel
	int w=800
	int h=600

	int idx=0
	while idx<=(w*h*depth) do #appears to be BGR
		start[idx]=(idx%256)|byte
		start[idx+1]=((idx%256)-128)|byte
		start[idx+2]=100|byte
		kterm:print("\n@")
		kterm:dumphexi(idx)
		idx+=3
	done

	__khalt()
return